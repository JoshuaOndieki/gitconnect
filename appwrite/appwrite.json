{
    "projectId": "gitconnect",
    "projectName": "gitconnect",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "gitconnect",
            "name": "gitconnect",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "profiles",
            "$permissions": [],
            "databaseId": "gitconnect",
            "name": "profiles",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "avatar",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "format": "url",
                    "default": null
                },
                {
                    "key": "username",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 16,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "unique_userId",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "userId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "unique_username",
                    "type": "unique",
                    "status": "available",
                    "attributes": [
                        "username"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        }
    ]
}